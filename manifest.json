  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://manvue.com"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".webp"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+manvue",
      "url": "/product/%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "permissions": [
    "notifications",
    "background-sync",
    "storage"
  ],
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui"
  ],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b5",
  "scope_extensions": [
    {
      "origin": "https://api.manvue.com"
    }
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "developer": {
    "name": "MANVUE Team",
    "url": "https://manvue.com"
  },
  "version": "1.0.0",
  "minimum_version": "1.0.0"
